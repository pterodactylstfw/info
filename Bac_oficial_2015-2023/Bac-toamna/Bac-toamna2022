1.
void schimb(int &n, int x, int p)
{
    int putere=1;
    for(int i=1;i<=p;i++)
    {
        putere=putere*10;
    }
    int rest=n%putere;
    
    n/=putere*10;
    n=n*putere*10+x*putere;
    n+=rest;
}

2.
#include <iostream>
#include <fstream>
using namespace std;
int main()
{
    char s[50],voc[6]="aeiou";
    cin.getline(s,50);
    for(int i=0;i<strlen(s);i++)
    {
            while(strchr(voc,s[i])==NULL)
                s[i]=s[i]-1;
    }
    
    cout<<s;
}

3.
#include <iostream>
#include <fstream>
using namespace std;
int main()
{
    ifstream fin("numere.in");
    int x,y,nr=0;
    fin>>y;
    int lg=1,lgmax=0;
    while(fin>>x)
    {
        if(x%2==y%2) lg++;
        else
        {
            if(lg>lgmax) {lgmax=lg;
                nr=1;}
            else if(lg==lgmax) nr++;
            lg=1;
            
        }
        y=x;
    }
    
    cout<<nr<<" "<<lgmax;
    
}