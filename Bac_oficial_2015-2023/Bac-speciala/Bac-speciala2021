1.

void imog(int x, int y, int &rez)
{
    rez=1;
    int nr1[10],nr2[10],ok1=0,ok2=0,i=0;
    while(x!=0)
    {
        nr1[i]=-1;
        int c=x%10;
        if(c%2==1)
        {
            ok1=1;
            nr1[i]=c;
        }
        x/=10;
        i++;

    }
    int n=i;i=0;
    while(y!=0)
    {
        nr2[i]=-1;
        int c=y%10;
        if(c%2==1)
        {
            ok2=1;
            nr2[i]=c;
        }
        y/=10;
        i++;

    }
    int m=i,j;i=0;
    
    if(ok1==0||ok2==0) rez=0;
     if(m!=n) rez=0;
    
        for(i=0,j=m;i<=n,j>=0;i++,j--)
        {
            cout<<nr1[i]<<" "<<nr2[j]<<endl;
            if(nr1[i]!=nr2[j])
        {
            rez=0;
            return;
        }}
    
    
}


2.

#include <iostream>
using namespace std;
int main()
{
    int n,k,a[101][101];
    cin>>n>>k;
    for(int i=1;i<=n;i++)
        for(int j=1;j<=n;j++)
            cin>>a[i][j];
    cout<<endl;
    
    
    int j=1;
    while(j<k) {swap(a[j][k],a[k][j]);
        j++;}
    
    for(int i=1;i<=n;i++)
    {for(int j=1;j<=n;j++)
        cout<<a[i][j]<<" ";
        cout<<endl;}
}

3.

#include <fstream>
#include <iostream>
using namespace std;
ifstream fin("numere.in");
int main()
{
    int x,y;
    cin>>x>>y;
    cout<<y<<" ";
    int n=(y-x)/2;
    while(n!=0)
    {
        x=y-2*n;
        cout<<x<<" ";
        y=x;
        n--;
    }
}