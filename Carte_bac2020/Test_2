1.void perechi(int n, int v[],int &p)
{
    for(int i=1;i<n;i++)
    {
        
        for(int j=i+1;j<=n;j++)
        {int ok=1;
        for(int d=2;d<=min(v[i],v[j]);d++)
            if(v[i]%d==0&&v[j]%d==0) ok=0;
        if(ok==1) p++;}
    }
}


2.
#include <iostream>
#include <cstring>
using namespace std;
int main()
{
    int n;
    char c[21];
    cin>>n;
    cin.ignore();
    cin.getline(c,20);
    char s[30][201];
    for(int i=0;i<n;i++)
        cin.getline(s[i],200);
    
    for(int i=0;i<n;i++)
    {
        int ct=0;
        char *p=strtok(s[i]," ");
        while(p!=NULL)
        {
            char aux[25];
            strcpy(aux,p);
            for(int i=0;i<strlen(aux);i++)
                if(aux[i]>='A'&&aux[i]<='Z') aux[i]+=32;
            if(strncmp(aux,c,strlen(c))==0) ct++;
            p=strtok(NULL," ");
        }
        cout<<ct<<" ";
    }
}


3.
#include <fstream>
#include <iostream>
#include <cstring>
using namespace std;
ifstream fin("numere.in");
int main()
{
    int n,m,x,ok=0;
    fin>>n>>m;
    int v[1000]={0};
    for(int i=1;i<=n;i++)
        {fin>>x;
            v[x]=1;}
    
    for(int i=1;i<=m;i++)
    {
        fin>>x;
        if(v[x]==0) v[x]=2;
        ok=1;
        
    }
    if(ok==0)
    cout<<"Nu exista numere care vor fi inserate";
    else
    for(int i=1;i<=999;i++)
        if(v[i]==2) cout<<i<<" ";
    return 0;
}
