//Problema 4


//Problema 5
#include <iostream>
#include <cstring>
using namespace std;
int main()
{
     char s[101];
     cin.get(s, 100);
     int i, n = strlen(s);
     for (i = 0; i < n; i++)
     {
           if (s[i] == '<')
              while (s[i] != '>')
             {
                          if (isalpha(s[i]))
                                s[i] = toupper(s[i]);
                          i++;
              }
      }
      cout << s;
      return 0;
}

