2. F(2015,2);

3. subprogram int care sa returneze nr cifre prime

int prime(int n)
{
    int pr[5]={2,3,5,7,9};
    int ct=0;
    while(n)
    {
        for(int i=0;i<5;i++)
            if(n%10==pr[i]) {ct++;
                break;}
        n/=10;
    }
    return ct;
}

4.

#include <iostream>
#include <fstream>
using namespace std;
ifstream fin("numere.in");
int main()
{
    int n , x , s1 , s2 , s3 , s4;
    fin >> n;
    for(int i = 1 ; i <= n ; i ++)
    {
        fin >> x;
        if(x % 2 == 0) s1 += x;
        else s2 += x;
    }
    for(int i = 1 ; i <= n ; i ++)
    {
        fin >> x;
        if(x % 2 == 0) s3 += x;
        else s4 += x;
    }
    cout << s1 * s4 + s2 * s3 << endl;
}

