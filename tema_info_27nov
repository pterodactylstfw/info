1.#include <iostream>
#include <cstring>
using namespace std;
int main()
{
    char s[21][21];
    int n,lg,ct=0;
    cin>>n;
    cin.get();
    cin.getline(s[0],20);
    lg=strlen(s[0])-1;
    for(int i=1;i<n;i++)
    {
        cin.getline(s[i],20);
        if(strncmp(s[i],s[0],lg)==0) ct++;
    }
    
    cout<<ct;
    
}

