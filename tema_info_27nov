5.
#include <iostream>
#include <cstring>
using namespace std;
int main()
{
    char s[256];
    
    cin.getline(s,255);
    char *p=strtok(s," ");
    char aux[256];
    while(p!=NULL)
    {
        strcat(aux,p);
        strcat(aux, " ");
        p=strtok(NULL," ");
        
    }
    
    cout<<aux;
}

6.
#include <iostream>
#include <cstring>
using namespace std;
int main()
{
    char s[256];
    bool ok=false;
    cin.getline(s,255);
    char *p=strtok(s, " ");
    char aux[10];
    char sf[256];
    while(p!=NULL)
    {
        strcpy(aux,p);
        if(strlen(aux)>=3&&strlen(aux)%2==1)
        {
            ok=true;
            strcpy(aux+(strlen(aux)/2-1)+1,aux+(strlen(aux)/2-1)+2);
        }
        strcat(sf,aux);
        strcat(sf," ");
        p=strtok(NULL, " ");
    }
    cout<<sf;
}

